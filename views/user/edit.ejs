		<div class="col-md-8"><input <% if(user.idCardIssuanceCity) { %>value="<%= user.idCardIssuanceCity %>"<% } else { %>placeholder="<%= __("Issuance city") %>"<% } %> name="idCardIssuanceCity" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="idCardIssuanceCountry" class="col-md-4 control-label"><%= __('Issuance country') %></label>
		<div class="col-md-8"><input <% if(user.idCardIssuanceCountry) { %>value="<%= user.idCardIssuanceCountry %>"<% } else { %>placeholder="<%= __("Issuance country") %>"<% } %> name="idCardIssuanceCountry" type="text" class="form-control"/></div>
