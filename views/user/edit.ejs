<!-- Edit user bank account function used for views/user/edit.ejs -->
<script src="/nolinker/js/user/edit.js"></script>

<!-- Edit form -->
<form action="/user/update/<%= user.id %>" method="POST" class="form-horizontal form-user-edit form-validate">
	<div class="form-group">
		<div class="col-md-12">
			<h2><%= __('Edit profile') %></h2>
			<h2>
				<% if (session.editUserSection === 'basic') 		{ %> <%= __('Basic info') %><% } %>
				<% if (session.editUserSection === 'idCard')		{ %> <%= __('ID card') %><% } %>
				<% if (session.editUserSection === 'personalInfo') 	{ %> <%= __('Personal info') %><% } %>
			</h2>
		</div>
	</div>
	 
	<!-- Progress bar -->
	 <div class="form-group">
		<div class="col-md-12">
			<div class="progress">
	  			<div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
	   				60% <%= __('Completion') %>
	  			</div>
			</div>
		</div>
	</div>

	<% if(flash && flash.err) { %>
		<ul class="alert alert-success">
			<% Object.keys(flash.err ).forEach(function(error) { %>
			<li><%- JSON.stringify(flash.err[error]) %></li>
			<% }) %>
		</ul>
	<% } %>

	<!-- Basic Info -->
	<% if (session.editUserSection === 'basic') { %>
	<div class="form-group">
		<label for="title" class="col-md-4 control-label"><%= __('Title') %></label> 
		<div class="col-md-8">
		  <label class="radio-inline">
		    <input type="radio" name="title" value="Ms." <% if(user.title === 'Ms.') { %> checked <% } %> >
		   	<%= __('Ms.') %>
		  </label>
		  <label class="radio-inline">
		    <input type="radio" name="title" value="Miss" <% if(user.title === 'Miss') { %> checked <% } %> >
		   	<%= __('Miss') %>
		  </label>
		  <label class="radio-inline">
		    <input type="radio" name="title" value="Mr." <% if(user.title === 'Mr.') { %> checked <% } %> >
		   	<%= __('Mr.') %>
		  </label>
		</div>
	</div>
	<div class="form-group">
		<label for="firstname" class="col-md-4 control-label"><%= __('Firstname') %></label> 
		<div class="col-md-8"><input <% if(user.firstname) { %>value="<%= user.firstname %>"<% } else { %>placeholder="<%= __("Firstname") %>"<% } %> name="firstname" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="lastname" class="col-md-4 control-label"><%= __('Lastname') %></label> 
		<div class="col-md-8"><input <% if(user.lastname) { %>value="<%= user.lastname %>"<% } else { %>placeholder="<%= __("Lastname") %>"<% } %> name="lastname" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="maidenName" class="col-md-4 control-label"><%= __('Maiden name') %></label> 
		<div class="col-md-8"><input <% if(user.maidenName) { %>value="<%= user.maidenName %>"<% } else { %>placeholder="<%= __("Maiden name") %>"<% } %> name="maidenName" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="birthDate" class="col-md-4 control-label"><%= __('Date of birth') %></label> 
		<div class="col-md-8"><input <% if(user.birthDate) { %>value="<%= user.GetBirthDate() %>"<% } else { %>placeholder="<%= __("Date of birth") %>"<% } %> name="birthDate" type="text" class="form-control" id="datepickerBirthday"/></div>
	</div>
	<div class="form-group">
		<label for="birthPlace" class="col-md-4 control-label"><%= __('Birth place') %></label> 
		<div class="col-md-8"><input <% if(user.birthPlace) { %>value="<%= user.birthPlace %>"<% } else { %>placeholder="<%= __("Birth place") %>"<% } %> name="birthPlace" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="birthDepartment" class="col-md-4 control-label"><%= __('Department of birth place') %></label> 
		<div class="col-md-8"><input <% if(user.birthDepartment) { %>value="<%= user.birthDepartment %>"<% } else { %>placeholder="<%= __("Department of birth place") %>"<% } %> name="birthDepartment" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="addressStreetNumber" class="col-md-4 control-label"><%= __('Street number') %></label> 
		<div class="col-md-8"><input <% if(user.addressStreetNumber) { %>value="<%= user.addressStreetNumber %>"<% } else { %>placeholder="<%= __("Street number") %>"<% } %> name="addressStreetNumber" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="addressStreet" class="col-md-4 control-label"><%= __('Address') %></label> 
		<div class="col-md-8"><input <% if(user.addressStreet) { %>value="<%= user.addressStreet %>"<% } else { %>placeholder="<%= __("Address") %>"<% } %> name="addressStreet" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="postalCode" class="col-md-4 control-label"><%= __('Postal code') %></label> 
		<div class="col-md-8"><input <% if(user.postalCode) { %>value="<%= user.postalCode %>"<% } else { %>placeholder="<%= __("Postal code") %>"<% } %> name="postalCode" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="city" class="col-md-4 control-label"><%= __('City') %></label> 
		<div class="col-md-8"><input <% if(user.city) { %>value="<%= user.city %>"<% } else { %>placeholder="<%= __("City") %>"<% } %> name="city" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="country" class="col-md-4 control-label"><%= __('Country') %></label> 
		<div class="col-md-8"><input <% if(user.country) { %>value="<%= user.country %>"<% } else { %>placeholder="<%= __("Country") %>"<% } %> name="country" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="phoneNumberHome" class="col-md-4 control-label"><%= __('Home phone') %></label>
		<div class="col-md-8"><input <% if(user.phoneNumberHome) { %>value="<%= user.phoneNumberHome %>"<% } else { %>placeholder="<%= __("Home phone") %>"<% } %> name="phoneNumberHome" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="phoneNumberMobile" class="col-md-4 control-label"><%= __('Mobile phone') %></label>
		<div class="col-md-8"><input <% if(user.phoneNumberMobile) { %>value="<%= user.phoneNumberMobile %>"<% } else { %>placeholder="<%= __("Mobile phone") %>"<% } %> name="phoneNumberMobile" type="text" class="form-control"/></div>
	</div>
	<% } %>

	<!-- Id card -->
	<% if (session.editUserSection === 'idCard') { %>
	<div class="form-group">
		<label for="idCardType" class="col-md-4 control-label"><%= __('Type') %></label>
		<div class="col-md-8"><input <% if(user.idCardType) { %>value="<%= user.idCardType %>"<% } else { %>placeholder="<%= __("Type") %>"<% } %> name="idCardType" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="idCardExpirationDate" class="col-md-4 control-label"><%= __('Expiration date') %></label>
		<div class="col-md-8"><input <% if(user.idCardExpirationDate) { %>value="<%= user.GetIdCardExpirationDate() %>"<% } else { %>placeholder="<%= __("Expiration date") %>"<% } %> name="idCardExpirationDate" type="text" class="form-control datepicker"/></div>
