			<div class="radio">
			  <label>
			    <input type="radio" name="familyAssetsRange" value="5Mto15M" <% if(user.familyAssetsRange === '5Mto15M') { %> checked <% } %> >
			   	<%= __('from') %> 5M€ <%= __('to') %> 15M€
			  </label>
			</div>
			<div class="radio">
			  <label>
			    <input type="radio" name="familyAssetsRange" value="above15M" <% if(user.familyAssetsRange === 'above15M') { %> checked <% } %> >
			   	<%= __('above') %> 15M€
			  </label>
			</div>
		</div>
