	<div class="form-group">
		<label for="idCardIssuanceAuthority" class="col-md-4 control-label"><%= __('Issuance authority') %></label>
		<div class="col-md-8"><input <% if(user.idCardIssuanceAuthority) { %>value="<%= user.idCardIssuanceAuthority %>"<% } else { %>placeholder="<%= __("Issuance authority") %>"<% } %> name="idCardIssuanceAuthority" type="text" class="form-control"/></div>
	</div>
	<div class="form-group">
		<label for="nationality" class="col-md-4 control-label"><%= __('Nationality') %></label>
		<div class="col-md-8"><input <% if(user.nationality) { %>value="<%= user.nationality %>"<% } else { %>placeholder="<%= __("Nationality") %>"<% } %> name="nationality" type="text" class="form-control"/></div>
	</div>
	<% } %>

